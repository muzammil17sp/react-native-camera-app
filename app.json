{
  "expo": {
    "name": "Games",
    "slug": "games",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/camera.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow React Native to access your location"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.muzammil17sp.games"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
